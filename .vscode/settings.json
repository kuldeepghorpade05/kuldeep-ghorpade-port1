{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/node_modules": false
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true
    },
    "explorer.excludeGitIgnore": false,
    "javascript.implicitProjectConfig.checkJs": true,
    "javascript.suggest.autoImports": true,
    "python.analysis.extraPaths": [
        "./backend"
    ],
    "python.defaultInterpreterPath": "./backend/venv/bin/python",
    "editor.outline.showFunctions": true,
    "editor.outline.showVariables": true,
    "editor.outline.showClasses": true,
    "editor.outline.showIcons": true
}